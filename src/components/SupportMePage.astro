---
// Compatibility wrapper: render the new SupportUsPage when older SupportMePage is imported
import SupportUsPage from "./SupportUsPage.astro";
import type { SupportUsLocaleContent } from "../data/supportUs.i18n";

export interface Props {
  lang: 'en' | 'es' | 'fr';
  t: SupportUsLocaleContent;
}

const { t } = Astro.props as Props;
---

<SupportUsPage lang={t ? Astro.props.lang : 'en'} t={t} />
